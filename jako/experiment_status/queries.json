{
"total_nodes": "query MyQuery {
  %(experiment_name)s(limit: 1, order_by: {total_nodes: desc}) {
    total_nodes
  }
}",

"number_of_permutations":"
query MyQuery {
  %(experiment_name)s_aggregate {
    aggregate {
      max {
        experiment_stage
      }
    }
  }
  %(experiment_name)s {
    experiment_id
  }
}
",

"max_by_metric":"
query MyQuery {

  %(experiment_name)s_aggregate {
    aggregate {
      max {
        %(metric)s
      }
    }
  }
}
",

"min_by_metric":"
query MyQuery {

  %(experiment_name)s_aggregate {
    aggregate {
      min {
        %(metric)s
      }
    }
  }
}
",

"max_by_parameter":"
query MyQuery {

  %(experiment_name)s_aggregate {
    aggregate {
      max {
        %(parameter)s
      }
    }
  }
}
",
}