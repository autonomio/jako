{"latest_experiment_stage": '''query MyQuery {  %(experiment_name)s_aggregate {    aggregate {      max {        experiment_stage      }    }  }}''',"total_nodes": '''query MyQuery {  %(experiment_name)s(limit: 1, order_by: {total_nodes: desc}) {    total_nodes  }}''',"number_of_permutations":'''query MyQuery {  %(experiment_name)s(where: {experiment_stage: {_eq: "%(stage)d"}}) {    experiment_id  }}''',"max_by_metric":'''query MyQuery {  %(experiment_name)s_aggregate(where: {experiment_stage: {_eq: "%(stage)d"}}) {    aggregate {      max {        %(metric)s      }    }  }}''',"min_by_metric":'''query MyQuery {  %(experiment_name)s_aggregate (where: {experiment_stage: {_eq: "%(stage)d"}}){    aggregate {      min {        %(metric)s      }    }  }}''',"max_by_parameter":'''query MyQuery {  %(experiment_name)s_aggregate(where: {experiment_stage: {_eq: "%(stage)d"}, %(parameter)s: {_eq: "%(param_value)s"}}) {    aggregate {      max {        %(metric)s      }    }  }}''',"min_by_parameter":'''query MyQuery {  %(experiment_name)s_aggregate(where: {experiment_stage: {_eq: "%(stage)d"}, %(parameter)s: {_eq: "%(param_value)s"}}) {    aggregate {      min {        %(metric)s      }    }  }}''',"time_per_permutation":'''query MyQuery {  %(experiment_name)s(limit: 2, order_by: {timestamp: desc}) {    timestamp  }}''',"total_time":'''query MyQuery {  %(experiment_name)s(order_by: {timestamp: desc}, limit: 1) {    timestamp  }  %(experiment_name)s_aggregate(where: {experiment_stage: {_eq: "%(stage)d"}}) {    aggregate {      min {        timestamp      }    }  }}''',"params_by_max_metric":'''query MyQuery {  %(experiment_name)s(where: {%(metric)s: {_eq: "%(max_metric).20f"}, _and: {experiment_stage: {_eq: "%(stage)d"}}}) {    %(parameter)s  }}''',"params_by_min_metric":'''query MyQuery {  %(experiment_name)s(where: {%(metric)s: {_eq: "%(min_metric).20f"}, _and: {experiment_stage: {_eq: "%(stage)d"}}}) {    %(parameter)s  }}'''}